    <script type="text/javascript">
        var editIndexTable_associatedproducts_grid = -1;
        var editRowData_associatedproducts_grid = [];
        var columnData_associatedproducts_grid = [];

        var obj = {
            'Data': 'DisplayOrder',
            'Editable': true,
            'Type': 'number'
        }
        columnData_associatedproducts_grid.push(obj);

        function editData_associatedproducts_grid(dataId, data) {
            var modData = data;
            if (typeof data == 'string') {
                modData = data.replace(/[.*+?^${}()|[\]\\]/g, "_");
            }
            $('#buttonEdit_associatedproducts_grid' + modData).hide();
            $('#buttonConfirm_associatedproducts_grid' + modData).show();
            $('#buttonCancel_associatedproducts_grid' + modData).show();
            rowEditMode_associatedproducts_grid(dataId);
        }

        function rowEditMode_associatedproducts_grid(rowid) {
            var prevRow;
            var rowIndexVlaue = parseInt(rowid[0].rowIndex);
            if (editIndexTable_associatedproducts_grid == -1) {
                saveRowIntoArray_associatedproducts_grid(rowid);
                rowid.attr("editState", "editState");
                editIndexTable_associatedproducts_grid = rowid.rowIndex;
                setEditStateValue_associatedproducts_grid(rowid);
            }
            else {
                prevRow = $("[editState=editState]");
                prevRow.attr("editState", "");
                rowid.attr("editState", "editState");
                editIndexTable_associatedproducts_grid = rowIndexVlaue;
                saveArrayIntoRow_associatedproducts_grid(prevRow);
                saveRowIntoArray_associatedproducts_grid(rowid);
                setEditStateValue_associatedproducts_grid(rowid);
            }
        }

        function escapeQuotHtml(value) {
            return String(value).replace(/["]/g, function (s) {
                return '&quot;';
            });
        }

        function setEditStateValue_associatedproducts_grid(curRow) {
            for (var cellName in editRowData_associatedproducts_grid) {
                var columnType = 'string';

                $.each(columnData_associatedproducts_grid, function (index, element) {
                    if (element.Data == cellName) {
                        columnType = element.Type;
                    }
                });

                if (columnType == 'number') {
                    $($(curRow).children("[data-columnname='" + cellName + "']")[0]).html('<input value="' + editRowData_associatedproducts_grid[cellName] + '" class="userinput" type="number" step="any" min="-2147483648" max="2147483647"/>');
                }
                if (columnType == 'checkbox') {
                    var cellValue = editRowData_associatedproducts_grid[cellName];
                    if ($(cellValue).attr('nop-value') === 'true') {
                        $($(curRow).children("[data-columnname='" + cellName + "']")[0]).html('<input value="true" checked class="userinput" type="checkbox" onclick="checkBoxClick_associatedproducts_grid(this)"/>');
                    }
                    else {
                        $($(curRow).children("[data-columnname='" + cellName + "']")[0]).html('<input value="false" class="userinput" type="checkbox" onclick="checkBoxClick_associatedproducts_grid(this)"/>');
                    }
                }
                if (columnType == 'string') {
                    var strValue = editRowData_associatedproducts_grid[cellName];
                    $($(curRow).children("[data-columnname='" + cellName + "']")[0]).html('<input value="' + escapeQuotHtml(strValue) + '" class="userinput"  style="width: 99% " />');
                }
                $('#associatedproducts-grid').DataTable().columns.adjust();
            }
        }

        function checkBoxClick_associatedproducts_grid(checkBox) {
            var input = $(checkBox);
            if ($(input).val() === 'true') {
                $(input).val('false');
                $(input).removeAttr('checked');
            } else {
                $(input).val('true');
                $(input).attr('checked', 'checked');
            }
        }

        function confirmEditData_associatedproducts_grid(dataId, data, nameData) {
            var origData = data;
            var modData = data;
            if (typeof data == 'string') {
                modData = data.replace(/[.*+?^${}()|[\]\\]/g, "_");
            }
            $('#buttonEdit_associatedproducts_grid' + modData).show();
            $('#buttonConfirm_associatedproducts_grid' + modData).hide();
            $('#buttonCancel_associatedproducts_grid' + modData).hide();

            updateRowData_associatedproducts_grid(dataId, origData, nameData);
        }

        function updateRowData_associatedproducts_grid(currentCells, data, nameData) {
            var updateRowData = [];
            updateRowData.push({ 'pname': nameData, 'pvalue': data });
            $.each(columnData_associatedproducts_grid, function (index, element) {
                if (element.Editable == true) {
                    var value = $($($(currentCells).children("[data-columnname='" + element.Data + "']")).children('input')[0]).val();
                    updateRowData.push({
                        'pname': element.Data, 'pvalue': value
                    });
                }
            });
            var postData = {};
            for (i = 0; i < updateRowData.length; i++) {
                postData[updateRowData[i].pname] = updateRowData[i].pvalue;
            }
            var tokenInput = $('input[name=__RequestVerificationToken]').val();
            postData['__RequestVerificationToken'] = tokenInput;
            addAntiForgeryToken(postData);

            $.ajax({
                url: "/Admin/Product/AssociatedProductUpdate",
                type: "POST",
                dataType: "json",
                data: postData,
                success: function (data, textStatus, jqXHR) {
                    //display error if returned
                    if (data) {
                        display_nop_error(data);
                    }
                    //refresh grid
                    $('#associatedproducts-grid').DataTable().draw(false);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    alert(errorThrown);
                }
            });
        }

        function cancelEditData_associatedproducts_grid(dataId, data) {
            var modData = data;
            if (typeof data == 'string') {
                modData = data.replace(/[.*+?^${}()|[\]\\]/g, "_");
            }
            $('#buttonEdit_associatedproducts_grid' + modData).show();
            $('#buttonConfirm_associatedproducts_grid' + modData).hide();
            $('#buttonCancel_associatedproducts_grid' + modData).hide();

            var prevRow = $("[editState=editState]");
            prevRow.attr("editState", "");
            if (prevRow.length > 0) {
                saveArrayIntoRow_associatedproducts_grid($(prevRow));
            }
            editIndexTable_associatedproducts_grid = -1;
        }

        function saveArrayIntoRow_associatedproducts_grid(cureentCells) {
            for (var cellName in editRowData_associatedproducts_grid) {
                $($(cureentCells).children("[data-columnname='" + cellName + "']")[0]).html(editRowData_associatedproducts_grid[cellName]);
            }
        }

        function saveRowIntoArray_associatedproducts_grid(cureentCells) {
            $.each(columnData_associatedproducts_grid, function (index, element) {
                if (element.Editable == true) {
                    var htmlVal = $($(cureentCells).children("[data-columnname='" + element.Data + "']")[0]).html();
                    editRowData_associatedproducts_grid[element.Data] = htmlVal;
                }
            });
        }
    </script>
