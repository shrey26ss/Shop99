        }
        columnData_productpictures_grid.push(obj);

        function editData_productpictures_grid(dataId, data) {
            var modData = data;
            if (typeof data == 'string') {
                modData = data.replace(/[.*+?^${}()|[\]\\]/g, "_");
            }
            $('#buttonEdit_productpictures_grid' + modData).hide();
            $('#buttonConfirm_productpictures_grid' + modData).show();
            $('#buttonCancel_productpictures_grid' + modData).show();
            rowEditMode_productpictures_grid(dataId);
        }

        function rowEditMode_productpictures_grid(rowid) {
            var prevRow;
            var rowIndexVlaue = parseInt(rowid[0].rowIndex);
            if (editIndexTable_productpictures_grid == -1) {
                saveRowIntoArray_productpictures_grid(rowid);
                rowid.attr("editState", "editState");
                editIndexTable_productpictures_grid = rowid.rowIndex;
                setEditStateValue_productpictures_grid(rowid);
            }
            else {
                prevRow = $("[editState=editState]");
                prevRow.attr("editState", "");
                rowid.attr("editState", "editState");
                editIndexTable_productpictures_grid = rowIndexVlaue;
                saveArrayIntoRow_productpictures_grid(prevRow);
                saveRowIntoArray_productpictures_grid(rowid);
                setEditStateValue_productpictures_grid(rowid);
            }
        }

        function escapeQuotHtml(value) {
            return String(value).replace(/["]/g, function (s) {
                return '&quot;';
            });
        }

        function setEditStateValue_productpictures_grid(curRow) {
            for (var cellName in editRowData_productpictures_grid) {
                var columnType = 'string';

                $.each(columnData_productpictures_grid, function (index, element) {
                    if (element.Data == cellName) {
                        columnType = element.Type;
                    }
                });

                if (columnType == 'number') {
                    $($(curRow).children("[data-columnname='" + cellName + "']")[0]).html('<input value="' + editRowData_productpictures_grid[cellName] + '" class="userinput" type="number" step="any" min="-2147483648" max="2147483647"/>');
                }
                if (columnType == 'checkbox') {
                    var cellValue = editRowData_productpictures_grid[cellName];
                    if ($(cellValue).attr('nop-value') === 'true') {
                        $($(curRow).children("[data-columnname='" + cellName + "']")[0]).html('<input value="true" checked class="userinput" type="checkbox" onclick="checkBoxClick_productpictures_grid(this)"/>');
                    }
                    else {
                        $($(curRow).children("[data-columnname='" + cellName + "']")[0]).html('<input value="false" class="userinput" type="checkbox" onclick="checkBoxClick_productpictures_grid(this)"/>');
                    }
                }
                if (columnType == 'string') {
                    var strValue = editRowData_productpictures_grid[cellName];
                    $($(curRow).children("[data-columnname='" + cellName + "']")[0]).html('<input value="' + escapeQuotHtml(strValue) + '" class="userinput"  style="width: 99% " />');
                }
                $('#productpictures-grid').DataTable().columns.adjust();
            }
        }

        function checkBoxClick_productpictures_grid(checkBox) {
            var input = $(checkBox);
            if ($(input).val() === 'true') {
                $(input).val('false');
                $(input).removeAttr('checked');
            } else {
                $(input).val('true');
                $(input).attr('checked', 'checked');
            }
        }

        function confirmEditData_productpictures_grid(dataId, data, nameData) {
            var origData = data;
            var modData = data;
            if (typeof data == 'string') {
                modData = data.replace(/[.*+?^${}()|[\]\\]/g, "_");
            }
            $('#buttonEdit_productpictures_grid' + modData).show();
            $('#buttonConfirm_productpictures_grid' + modData).hide();
            $('#buttonCancel_productpictures_grid' + modData).hide();

            updateRowData_productpictures_grid(dataId, origData, nameData);
        }

        function updateRowData_productpictures_grid(currentCells, data, nameData) {
            var updateRowData = [];
            updateRowData.push({ 'pname': nameData, 'pvalue': data });
            $.each(columnData_productpictures_grid, function (index, element) {
                if (element.Editable == true) {
                    var value = $($($(currentCells).children("[data-columnname='" + element.Data + "']")).children('input')[0]).val();
                    updateRowData.push({
                        'pname': element.Data, 'pvalue': value
                    });
                }
            });
            var postData = {};
            for (i = 0; i < updateRowData.length; i++) {
                postData[updateRowData[i].pname] = updateRowData[i].pvalue;
            }
            var tokenInput = $('input[name=__RequestVerificationToken]').val();
            postData['__RequestVerificationToken'] = tokenInput;
            addAntiForgeryToken(postData);

            $.ajax({
                url: "/Admin/Product/ProductPictureUpdate",
                type: "POST",
                dataType: "json",
                data: postData,
                success: function (data, textStatus, jqXHR) {
                    //display error if returned
                    if (data) {
                        display_nop_error(data);
                    }
                    //refresh grid
                    $('#productpictures-grid').DataTable().draw(false);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    alert(errorThrown);
                }
            });
        }

        function cancelEditData_productpictures_grid(dataId, data) {
            var modData = data;
            if (typeof data == 'string') {
                modData = data.replace(/[.*+?^${}()|[\]\\]/g, "_");
            }
            $('#buttonEdit_productpictures_grid' + modData).show();
            $('#buttonConfirm_productpictures_grid' + modData).hide();
            $('#buttonCancel_productpictures_grid' + modData).hide();

            var prevRow = $("[editState=editState]");
            prevRow.attr("editState", "");
            if (prevRow.length > 0) {
                saveArrayIntoRow_productpictures_grid($(prevRow));
            }
            editIndexTable_productpictures_grid = -1;
        }

        function saveArrayIntoRow_productpictures_grid(cureentCells) {
            for (var cellName in editRowData_productpictures_grid) {
                $($(cureentCells).children("[data-columnname='" + cellName + "']")[0]).html(editRowData_productpictures_grid[cellName]);
