        var editIndexTable_productpictures_grid = -1;
        var editRowData_productpictures_grid = [];
        var columnData_productpictures_grid = [];

        var obj = {
            'Data': 'DisplayOrder',
            'Editable': true,
            'Type': 'number'
        }
        columnData_productpictures_grid.push(obj);

        var obj = {
            'Data': 'OverrideAltAttribute',
