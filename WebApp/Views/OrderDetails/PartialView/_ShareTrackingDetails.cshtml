@model WebApp.Models.ViewModels.TrackingModel


<div class="card">
    <div class="card-body">
        <div class="row justify-content-center">
            <div class="col-sm-12 text-center">
                <div class="form-group">
                    <input type="text" class="form-control" asp-for="URL" />
                </div>
                <div class="form-group">
                    <button class="btn btn-sm btn-outline-success"><i class="fa fa-whatsapp" aria-hidden="true"></i></button>
                    <button class="btn btn-sm btn-outline-danger"><i class="fa fa-envelope" aria-hidden="true"></i></i></button>
                    <button class="btn btn-sm btn-outline-info"><i class="fa fa-phone-square" aria-hidden="true"></i></button>
                    
                    
                    
                </div>
            </div>
        </div>
    </div>
</div>